<?xml version="1.0"?>
<package format="2">
  <name>aws_robomaker_simulations_hexagon</name>
  <version>1.0.0</version>
  <description>
    AWS RoboMaker Gazebo simulation package for a small hexagon with images.
  </description>
  <license>Apache 2.0</license>
  <author email="gavinaws@amazon.com">Gavin Adams</author>
  <maintainer email="gavinaws@amazon.com">Gavin Adams</maintainer>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>gazebo_ros</depend>
  <exec_depend>gazebo</exec_depend>
  <exec_depend>gazebo_plugins</exec_depend>
  <export>
    <gazebo_ros plugin_path="${prefix}/lib" gazebo_media_path="${prefix}" gazebo_model_path="${prefix}/models"/>
  </export>
</package>
